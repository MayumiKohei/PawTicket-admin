{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "nodejs20",
      "region": "us-central1"
    }
  ],
  "hosting": {
    "public": "public",
    "ignore": [ "firebase.json", "**/.*", "**/node_modules/**" ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "nextjsfunc",
          "region": "us-central1"
        }
      }
    ]
  }
}
